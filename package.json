{
  "name": "galeon-hospitals-map",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NEXT_WEBPACK_DISABLE_CACHE=true next dev",
    "dev:cf": "bash dev-gitbash.sh",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" next build",
    "post-build": "node scripts/post-build.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:e2e": "playwright test",
    "prepare": "npx husky",
    "deploy": "bash deploy-gitbash.sh",
    "deploy:cf": "npx wrangler pages deploy out --project-name=galeon-community-map",
    "clean": "rimraf .next out node_modules/.cache",
    "restart": "taskkill //F //IM node.exe //T && npm run dev",
    "build:dev": "env-cmd -f .env.development cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NEXT_LINT_IGNORE_WARNINGS=true next build || true",
    "build:prod": "env-cmd -f .env.production cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NEXT_LINT_IGNORE_WARNINGS=true next build || true",
    "export": "next export || true",
    "deploy:cf-dev": "bash deploy-gitbash.sh dev",
    "deploy:cf-prod": "bash deploy-gitbash.sh prod",
    "analyze": "cross-env ANALYZE=true npm run build"
  },
  "dependencies": {
    "@lingui/conf": "^5.2.0",
    "@lingui/core": "^5.2.0",
    "@lingui/format-json": "^5.2.0",
    "@lingui/macro": "^5.2.0",
    "@lingui/react": "^5.2.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.66.11",
    "@tanstack/react-query-devtools": "^5.66.11",
    "@types/file-saver": "^2.0.7",
    "@types/jspdf": "^1.3.3",
    "@types/screenfull": "^4.0.0",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "make-plural": "^7.4.0",
    "mapbox-gl": "^3.10.0",
    "next": "^15.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-use-gesture": "^9.1.3",
    "screenfull": "^6.0.2",
    "web-vitals": "^5.0.1",
    "zod": "^4.0.17",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3.0.1",
    "@lingui/cli": "^5.2.0",
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.50.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.2.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "tailwindcss": "^4.1.12",
    "typescript": "^5",
    "vitest": "^3.0.7",
    "workbox-webpack-plugin": "^7.3.0",
    "workbox-window": "^7.3.0"
  }
}
