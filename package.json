{
  "name": "galeon-hospitals-map",
  "version": "0.2.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NEXT_WEBPACK_DISABLE_CACHE=true next dev",
    "dev:cf": "bash dev-gitbash.sh",
    "dev:fix": "node dev-fix.js",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" next build",
    "post-build": "node scripts/post-build.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:e2e": "playwright test",
    "prepare": "npx husky",
    "deploy": "bash deploy-gitbash.sh",
    "deploy:cf": "npx wrangler pages deploy out --project-name=galeon-community-map",
    "clean": "rimraf .next out node_modules/.cache",
    "restart": "taskkill //F //IM node.exe //T && npm run dev",
    "build:dev": "env-cmd -f .env.development cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NEXT_LINT_IGNORE_WARNINGS=true next build || true",
    "build:prod": "env-cmd -f .env.production cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NEXT_LINT_IGNORE_WARNINGS=true next build || true",
    "export": "next export || true",
    "deploy:cf-dev": "bash deploy-gitbash.sh dev",
    "deploy:cf-prod": "bash deploy-gitbash.sh prod",
    "optimize": "node scripts/optimize-code.js",
    "optimize:analyze": "node scripts/optimize-code.js --analyze",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:server": "cross-env ANALYZE=true BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "cross-env ANALYZE=true BUNDLE_ANALYZE=browser npm run build",
    "bundle:analyze": "node scripts/bundle-analysis.js",
    "bundle:report": "npm run build && npm run bundle:analyze",
    "deps:analyze": "node scripts/dependency-analysis.js",
    "bundle:full": "npm run deps:analyze && npm run bundle:report",
    "bundle:optimize": "node scripts/bundle-optimization-report.js",
    "bundle:monitor": "node scripts/bundle-monitor.js",
    "bundle:watch": "npm run build && npm run bundle:monitor"
  },
  "dependencies": {
    "@lingui/conf": "5.4.1",
    "@lingui/core": "5.4.1",
    "@lingui/format-json": "5.4.1",
    "@lingui/macro": "5.4.1",
    "@lingui/react": "5.4.1",
    "@tanstack/react-query": "^5.66.11",
    "@tanstack/react-query-devtools": "^5.66.11",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/file-saver": "^2.0.7",
    "@types/jspdf": "^1.3.3",
    "@types/screenfull": "^4.0.0",
    "@use-gesture/react": "10.3.1",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "12.23.12",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "make-plural": "^7.4.0",
    "mapbox-gl": "^3.10.0",
    "next": "^15.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "screenfull": "^6.0.2",
    "web-vitals": "^5.0.1",
    "zod": "4.0.17",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@eslint/eslintrc": "^3.0.1",
    "@lingui/cli": "5.4.1",
    "@next/bundle-analyzer": "15.4.6",
    "@playwright/test": "^1.50.1",
    "@tailwindcss/postcss": "4.1.11",
    "@testing-library/jest-dom": "6.6.4",
    "@types/node": "24.2.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "@unocss/preset-wind": "^66.1.0-beta.3",
    "@unocss/webpack": "^66.1.0-beta.3",
    "@vitejs/plugin-react": "5.0.0",
    "autoprefixer": "10.4.21",
    "babel-loader": "^10.0.0",
    "cross-env": "10.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "9.33.0",
    "eslint-config-next": "15.4.6",
    "eslint-plugin-react-hooks": "5.2.0",
    "husky": "^9.0.11",
    "jsdom": "^26.0.0",
    "lint-staged": "16.1.5",
    "postcss": "8.5.6",
    "prettier": "^3.5.3",
    "rimraf": "6.0.1",
    "tailwindcss": "4.1.11",
    "typescript": "5.9.2",
    "unocss": "^66.1.0-beta.3",
    "vitest": "^3.0.7",
    "xlsx": "^0.18.5"
  }
}
