{
  "name": "galeon-hospitals-map",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.11.1",
    "npm": ">=10.2.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "prepare": "npx husky",
    "precommit": "npm run lint && npm run type-check",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "i18n": "npm run extract && npm run compile"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "tsc --noEmit"
    ]
  },
  "dependencies": {
    "@lingui/conf": "^5.2.0",
    "@lingui/core": "^5.2.0",
    "@lingui/format-json": "^5.2.0",
    "@lingui/macro": "^5.2.0",
    "@lingui/react": "^5.2.0",
    "@tanstack/react-query": "^5.66.11",
    "@tanstack/react-query-devtools": "^5.66.11",
    "@types/file-saver": "^2.0.7",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "make-plural": "^7.4.0",
    "mapbox-gl": "^3.10.0",
    "next": "15.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-responsive": "^10.0.0",
    "react-use-gesture": "^9.1.3",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.1",
    "@lingui/cli": "^5.2.0",
    "@playwright/test": "^1.50.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@unocss/postcss": "^66.1.0-beta.3",
    "@unocss/preset-rem-to-px": "^66.1.0-beta.3",
    "@unocss/preset-wind": "^66.1.0-beta.3",
    "@unocss/webpack": "^66.1.0-beta.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.2.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5",
    "unocss": "^66.1.0-beta.3",
    "vitest": "^3.0.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
