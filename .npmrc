# Désactiver la génération de package-lock.json
package-lock=false

# Désactiver la génération de fichiers de log
loglevel=error

# Désactiver l'audit de sécurité pendant l'installation
audit=false

# Désactiver la vérification des scripts
scripts-prepend-node-path=true

# Désactiver la génération de rapports d'erreur
report-metrics=false

# Désactiver la génération de fichiers de cache
cache=false

# Désactiver la génération de fichiers de debug
timing=false

# Désactiver la génération de fichiers de progress
progress=false

# Désactiver la génération de fichiers de fund
fund=false 