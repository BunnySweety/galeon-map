{
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "framework": "nextjs",
    "nodeVersion": "20",
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "buildOptions": {
      "env": {
        "NEXT_PUBLIC_MAPBOX_TOKEN": "${NEXT_PUBLIC_MAPBOX_TOKEN}"
      }
    },
    "crons": []
}