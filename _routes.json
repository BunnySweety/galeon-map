{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/_next/*",
    "/static/*",
    "/images/*",
    "/*.js",
    "/*.css",
    "/*.json",
    "/*.ico",
    "/*.png",
    "/*.jpg",
    "/*.svg",
    "/favicon.ico",
    "/robots.txt",
    "/sitemap.xml",
    "/manifest.json"
  ],
  "routes": [
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
} 