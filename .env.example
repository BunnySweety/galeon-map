# App
VITE_APP_NAME=Hospital Map
VITE_APP_VERSION=1.0.0
VITE_API_URL=https://api.example.com
VITE_MAP_TILE_URL=https://{s}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}{r}.png
VITE_MAP_ATTRIBUTION=© OpenStreetMap contributors, © CartoDB

# AWS
AWS_REGION=eu-west-1
AWS_S3_BUCKET=hospital-map-assets
AWS_CLOUDFRONT_ID=ABCDEF123456

# Analytics
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_SENTRY_DSN=https://xxxxx@xxx.ingest.sentry.io/xxxxx

# Features
VITE_ENABLE_AUTH=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SENTRY=true
VITE_ENABLE_SERVICE_WORKER=true

# API Keys
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key_here

# Cache
VITE_CACHE_TTL=3600
VITE_MAX_CACHE_SIZE=100

# Performance
VITE_MAX_MARKERS=1000
VITE_CLUSTER_THRESHOLD=100
VITE_ANIMATION_DURATION=300